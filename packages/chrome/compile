compile
	require
		--source=Ext.util.Observable
		--requires=Chrome.EventManager
	and
	union
		--file=core/root.js
		--recursive
	and
	exclude
		--namespace=Ext
	and
	include
		--tag=core
	and
	exclude
		--tag=dom
	and
	include
		--class=Ext.util.Event,Ext.util.DelayedTask
	and
	exclude
		--class=Ext.JSON,Ext.Template,Ext.XTemplate,Ext.XTemplateCompiler,Ext.XTemplateParser
	and
	meta
		--json
		--filenames
		--output=temp.txt